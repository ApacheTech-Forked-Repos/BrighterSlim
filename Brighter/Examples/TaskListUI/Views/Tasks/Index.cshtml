@model dynamic

@{    
    ViewBag.Title = "TodoUI - Brighter Example";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script id="viewTemplate" type="text/template">
    <ul id="taskList">
        {{#items}}
        <li>
            <div>
                @*<label>{{Rel}}</label>*@
                <label class="taskHref">{{href}}</label>
                <label>{{name}}</label>
                <label>{{description}}</label>
                <label>{{dueDate}}</label>
                <label>{{completionDate}}</label>
                <button id="HRef" class="complete">COMPLETE</button>
            </div>
        </li>
        {{/items}}
    </ul>
</script>

<h1>todos</h1>
<div id="todoList">
    <header>
        <h1>
            <input placeholder="Enter new task?"/>
            <button id="todoAdd">Add</button>
        </h1>
    </header>
    <section id="taskContainer">
    </section>
</div>

<script src="~/Scripts/jquery-2.1.3.js"></script>
<script src="~/Scripts/mustache.js"></script>
<script src="~/app/app.js"></script>


