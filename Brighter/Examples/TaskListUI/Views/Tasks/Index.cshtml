@model dynamic

@{
    ViewBag.Title = "TodoUI - Brighter Example";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script id="viewTemplate" type="text/template">
    <ul id="taskList">
        {{#items}}
        <li {{#isComplete}} class=completed {{/isComplete}}>
            <div>
                {{^isComplete}}
                <input type="checkbox" id="HRef" class="complete" />
                {{/isComplete}}
                <label class="taskHref" style="display:none">{{href}}</label>
                <label>{{name}}</label>
                <label>{{completionDate}}</label>
            </div>
        </li>
        {{/items}}
    </ul>
</script>

<h1>todos</h1>
<div id="todoList">
    <header>
        <input id="taskAddName" placeholder="Enter new task?"/>
        @*<button id="todoAdd">Add</button>*@
    </header>
    <section id="taskContainer">
    </section>
</div>

<script src="~/Scripts/jquery-2.1.3.min.js"></script>
<script src="~/Scripts/mustache.js"></script>
<script src="~/Scripts/bootstrap.min.js"></script>
<script src="~/app/app.js"></script>
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<link href="~/Content/Site.css" rel="stylesheet" />